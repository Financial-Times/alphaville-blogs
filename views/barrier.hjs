<!DOCTYPE html>
<html class="{{ctmCoreClass}}">
	<head>
		<title>{{ title }}</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />

		<link rel="stylesheet" href="{{assetsBasePath}}/build/main.css" />

		{{> ctm}}

		<script src="{{polyfillServiceUrl}}"></script>

		<script>
			(function () {
				ctmLoadScript({
					src: "{{assetsBasePath}}/build/main.js",
					async: true
				});
			}());
		</script>
	</head>
	<body>
	{{#headerConfig}}
		{{> header}}
	{{/headerConfig}}

<div class="o-grid-container">
    <div class="o-grid-row">
        <div class="main" data-o-grid-colspan="8">
            {{#barrierModel}}
                {{> barrier}}
            {{/barrierModel}}
        </div>
        <div class="rail" data-o-grid-colspan="4">
            {{>twitterWidget}}
        </div>
    </div>
</div>

	{{> footer}}

	</body>
</html>
